<div class="user-index-page">
  <div class="jumbotron user-index-content"
    <p id="notice"><%= notice %></p>

    <h1 class="shiny-new-user-header">Shiny New User</h1>

    <div class="new-user-q-btn">
      <%= link_to 'Are Ya New to S.O.S?', new_user_path %>
    </div>

    <% @users.each do |user| %>
    <div class="users-info">
      <div class="first-last-name">
        <li><strong>First Name:</strong> <%= user.first_name %></li>
        <li><strong>Last Name: </strong><%= user.last_name %></li>
      </div>
      <div class="users-show-edit-back">
        <%= link_to 'Show', user %> |
        <%= link_to 'Edit', edit_user_path(user) %> |
        <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
      </div>
    </div>
    <% end %>

    <div class="user-index-back">
        <%= link_to 'Back', products_path %>
    </div>
  </div>
</div>
